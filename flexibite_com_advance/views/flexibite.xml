            </xpath>
        </template>

         <template id="flexibite_com_advance.assets_themes">
            <t t-set="pos_theme_selector" t-value="request.env['ir.config_parameter'].sudo().get_param('theme_selector')"/>
            <t t-if="pos_theme_selector == False">
                <link rel="stylesheet" type="text/css" href="/flexibite_com_advance/static/src/css/restaurant_odoo_theme.css"/>
            </t>
            <t t-if="pos_theme_selector == 'multi_color'">
                <link rel="stylesheet" type="text/css" href="/flexibite_com_advance/static/src/css/restaurant_new.css"/>
            </t>
            <t t-if="pos_theme_selector == 'black_yellow'">
                <link rel="stylesheet" type="text/css" href="/flexibite_com_advance/static/src/css/restaurant_black_yellow.css"/>
            </t>
            <!-- <t t-if="pos_theme_selector == 'blue_white'">
                <link rel="stylesheet" type="text/css" href="/flexibite_com_advance/static/src/css/restaurant_blue_white.css"/>
            </t> -->
        </template>

        <template id="flexibite_com_advance.assets_gmaps">
            <t t-set="google_maps_api_key" t-value="request.env['ir.config_parameter'].sudo().get_param('google_api_key')"/>
            <script type="text/javascript" src="/flexibite_com_advance/static/src/js/lib/markerclusterer.js"/>
            <script t-att-async="1" t-att-defer="1" t-attf-src="//maps.googleapis.com/maps/api/js?key=#{google_maps_api_key}&amp;libraries=places"></script>
        </template>

