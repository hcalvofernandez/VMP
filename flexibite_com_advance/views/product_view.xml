<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="bags_prod" model="product.product">
        <field name="name">Bag</field>
        <field name="available_in_pos">True</field>
        <field name="list_price">5.00</field>
        <field name="type">consu</field>
        <field name="is_packaging">True</field>
        <field name="is_dummy_product">True</field>
        <field name="send_to_kitchen">False</field>
        <field name="taxes_id"/>
        <field name="supplier_taxes_id" />
        <field name="image_medium"></field>
    </record>

    <record id="delivery_charge" model="product.product">
        <field name="available_in_pos">True</field>
        <field name="list_price">0</field>
        <field name="is_dummy_product">True</field>
        <field name="send_to_kitchen">False</field>
        <field name="taxes_id"/>
        <field name="supplier_taxes_id" />
        <field name="type">service</field>
        <field name="name">Delivery Charge</field>
    </record>

    <record id="discount_product" model="product.product">
        <field name="available_in_pos">True</field>
        <field name="list_price">0</field>
        <field name="is_dummy_product">True</field>
        <field name="send_to_kitchen">False</field>
        <field name="taxes_id"/>
        <field name="supplier_taxes_id" />
        <field name="type">service</field>
        <field name="name">Discount Product</field>
    </record>

    <record id="acespritech_retail_payment_charge_product" model="product.product">
        <field name="name">Payment Charges</field>
        <field name="list_price">0</field>
        <field name="type">service</field>
        <field name="available_in_pos">True</field>
        <field name="is_dummy_product">True</field>
        <field name="send_to_kitchen">False</field>
        <field name="sale_ok">True</field>
        <field name="taxes_id"/>
        <field name="supplier_taxes_id" />
    </record>

    <record id="table_reservation_product" model="product.product">
        <field name="available_in_pos">True</field>
        <field name="is_dummy_product">True</field>
        <field name="send_to_kitchen">False</field>
        <field name="list_price">0</field>
        <field name="taxes_id"/>
        <field name="supplier_taxes_id" />
        <field name="type">service</field>
        <field name="name">Table Reservation Product</field>
    </record>

    <record id="package_product_template_only_form_view" model="ir.ui.view">
        <field name="name">package.product.template.form.view</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
        	<field name="to_weight" position="after">
           		<field name="loyalty_point"/>
           		<field name="non_refundable"/>
           		<field name="return_valid_days" attrs="{'invisible': [('non_refundable', '=', True)]}"/>
           	</field>
            <field name="sale_ok" position="before">
                <div>
                    <field name="is_packaging"/>
                    <label for="is_packaging"/>
                </div>
                <div>
                    <field name="is_combo"/>
                    <label for="is_combo"/>
                </div>
                <!-- <div>
                    <field name="send_to_kitchen"/>
                    <label for="send_to_kitchen"/>
                </div> -->
            </field>
            <notebook  position="inside">
                <page string="Modifier">
                    <field name="modifier_line">
                        <tree>
                            <field name="product_id"/>
                            <field name="price" readonly="1"/>
                            <field name="icon" widget=""/>
                            <field name="active"/>
                        </tree>
                    </field>
                </page>
            </notebook>
            <xpath expr='//page[4]' position="after" >
                <page string="Combo" attrs="{'invisible': [('is_combo', '=', False)]}">
                    <field name="product_combo_ids">
	                    <tree editable="bottom">
	                        <field name="require"/>
	                        <field name="pos_category_id" attrs="{'readonly': [('require', '=', True)]}" options="{'no_create':True}"/>
	                        <field name="product_ids" widget="many2many_tags" 
	                            context="{'is_required': require, 'category_from_line': pos_category_id}" options="{'no_create':True}"/>
	                        <field name="no_of_items" attrs="{'invisible': [('require', '=', True)]}"/>
	                    </tree>
	                </field>
                </page>
            </xpath>
            <field name="available_in_pos" position="after">
                <field name="make_time" placeholder="Minutes"/><div/>
            </field>
        </field>
    </record>

    <record model="ir.ui.view" id="aspl_product_template_form_view_invoice_policy">
        <field name="name">product.template.invoice.policy</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="sale.product_template_form_view_invoice_policy"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='invoicing']" position="before">
                <group string="Kitchen Configuration" name="kitchen_config" invisible="0">
                    <field name="send_to_kitchen"/>
                    <field name="priority" attrs="{'invisible': [('send_to_kitchen', '=', False)]}"/>
                </group>
            </xpath>
        </field>
    </record>

    <record id="aspl_pos_category_form" model="ir.ui.view">
        <field name="name">aspl.pos.category.form.view</field>
        <field name="model">pos.category</field>
        <field name="inherit_id" ref="point_of_sale.product_pos_category_form_view"/>
        <field name="arch" type="xml">
        	<field name="sequence" position="after">
        		<field name="loyalty_point" />
        		<field name="return_valid_days"/>
        	</field>
        </field>
    </record>

</odoo>